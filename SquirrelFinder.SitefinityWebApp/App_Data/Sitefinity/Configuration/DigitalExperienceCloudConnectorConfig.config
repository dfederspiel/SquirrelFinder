<?xml version="1.0" encoding="utf-8"?>
<digitalExperienceCloudConnectorConfig xmlns:config="urn:telerik:sitefinity:configuration" xmlns:type="urn:telerik:sitefinity:configuration:type" config:version="9.2.6227.0">
	<serverHandlers>
		<add predicate="Submit form" title="Submitted  form" group="Forms" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.FormEntrySubmittedTrackingHandler" />
		<add predicate="Create forum post under" title="Created forum post" group="Forums" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.ForumPostCreatedTrackingHandler" />
		<add predicate="Create forum thread" title="Created forum thread" group="Forums" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.ForumThreadCreatedTrackingHandler" />
		<add predicate="Download file" title="Downloaded  document" group="Files" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.MediaContentDownloadTrackingHandler" />
		<add predicate="Post comment" title="Created comment" group="Comments" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.CommentCreatedTrackingHandler" />
		<add predicate="Subscribe to forum thread" title="Subscribed to a forum thread" group="Forums" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.ForumThreadSubscriptionCreatedTrackingHandler" />
		<add predicate="Unsubscribe from forum thread" title="Unsubscribed from a forum thread" group="Forums" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.ForumThreadSubscriptionDeletedTrackingHandler" />
		<add predicate="Subscribe to forum" title="Subscribed to a forum" group="Forums" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.ForumSubscriptionCreatedTrackingHandler" />
		<add predicate="Unsubscribe from forum" title="Unsubscribed from a forum" group="Forums" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.ForumSubscriptionDeletedTrackingHandler" />
		<add predicate="Subscribe to newsletter" title="Subscribed to email campaign" group="Email campaigns" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.NewsletterSubscriptionCreatedTrackingHandler" />
		<add predicate="Unsubscribe from newsletter" title="Unsubscribed from email campaign" group="Email campaigns" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.NewsletterSubscriptionDeletedTrackingHandler" />
		<add predicate="Login" title="User logged in" group="Users" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.UserLoginHandler" />
		<add predicate="View personalized page" title="Visited personalized page" group="Pages" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.PersonalizedPageTrackingHandler" />
		<add predicate="View personalized widget" title="Visited personalized widget" group="Pages" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.PersonalizedWidgetTrackingHandler" />
		<add predicate="Register" title="User registered" group="Users" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.UserRegisteredTrackingHandler" />
		<add predicate="Activate user" title="User activated account" group="Users" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.UserActivatedTrackingHandler" />
	</serverHandlers>
	<mappingHandlers>
		<add title="User mapping (map user with activities upon login)" group="Users" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.LoginMappingHandler" />
	</mappingHandlers>
	<subjectMetadataHandlers>
		<add title="User metadata" group="Users" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.UserCreatedTrackingHandler" />
		<add title="User profile metadata" group="Users" handlerType="Telerik.Sitefinity.DataIntelligenceConnector.EventHandlers.UserProfileCreatedTrackingHandler" />
	</subjectMetadataHandlers>
	<personalizationCriterions>
		<add description="Personalization segments can be based on Campaigns created in Digital Experience Cloud" name="CampaignCriterion" />
		<add description="Personas created in Digital Experience Cloud are added as a criteria in page personalization" name="PersonaCriterion" />
	</personalizationCriterions>
</digitalExperienceCloudConnectorConfig>